select a.COMP_ID 公司ID
,a.COMP_NAME 公司名称,COMP_SNAME 公司中文简称,ADDRESS 注册地址,PROVINCE 省份,CITY 城市
,ZIPCODE 邮编,PHONE 电话,FAX 传真,EMAIL 电子邮箱,FOUNDDATE 成立日期,REGCAPITAL "注册资本(万元)"
,WEBSITE 公司网址,REGISTERNUMBER 统一社会信用代码
,b.COMP_NAME 公司曾用名
from GTJACOMPINTRODUCTION a
left join GTJACOMPPREVIOUSNAME b on a.COMP_ID=b.COMP_ID